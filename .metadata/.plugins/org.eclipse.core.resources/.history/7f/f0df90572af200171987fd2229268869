# Python Palindromes
entered_string = input("Enter a Palindrome - ")
string_array = entered_string.split()
reversed_string = string_array
# array_length
# array_iterator
reversed_string = []
length_of_string = 0
for value in string_array:
    ++length_of_string
for value in string_array:
        reversed_string[length_of_string - 1] = value

print(reversed_string)

